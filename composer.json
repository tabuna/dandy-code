    "require": {
        "akh/typograf": "^0.7.0",
        "illuminate/collections": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/support": "^12.0",
        "mpdf/mpdf": "^8.2",
        "spatie/commonmark-highlighter": "^3.0",
        "symfony/console": "^7.3",
        "vanderlee/syllable": "^1.9",
